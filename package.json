{
  "name": "vscode-auto-scroll",
  "displayName": "AutoScroll",
  "description": "auto scroll to the end of file",
  "version": "1.1.0",
  "publisher": "pejmannikram",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/PejmanNik/vscode-autoScroll.git"
  },
  "keywords": [
    "Log",
    "Log File",
    "Scroll",
    "Auto Scroll",
    "End of file"
  ],
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:log",
    "onCommand:autoScroll.enable"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autoScroll.disable",
        "title": "Disable Auto Scroll",
        "category": "Auto Scroll"
      },
      {
        "command": "autoScroll.enable",
        "title": "Enable Auto Scroll",
        "category": "Auto Scroll"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "AutoScroll",
      "properties": {
        "autoscroll.keepLastLineInCenter": {
          "type": "boolean",
          "default": true,
          "description": "Keep last line in center of view."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "ts-loader": "^9.2.6",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.2"
  }
}